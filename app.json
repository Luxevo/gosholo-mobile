{
  "expo": {
    "name": "gosholo-mobile",
    "slug": "gosholo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios-icon.png",
    "scheme": "gosholomobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jostinjarry1.gosholomobile",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app uses location to show nearby restaurants and food events on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to show nearby restaurants and food events on the map."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZ29zaG9sb2RldiIsImEiOiJjbWU5NWptYXMwbHdqMmttcXVxbTdhajNrIn0.l0kt4ycQbI8Qiz2s_77NNQ"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2bab544a-7b3d-4844-8a22-4d92909ec243"
      }
    }
  }
}
