{
  "expo": {
    "name": "gosholo",
    "slug": "gosholo-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios-icon.png",
    "scheme": "gosholomobile",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jostinjarry1.gosholomobile",
      "buildNumber": "32",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app uses location to show nearby restaurants and food events on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to show nearby restaurants and food events on the map.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to share images to social media."
      },
      "associatedDomains": ["applinks:app.gosholo.com"]
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.gosholo.gosholo",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.gosholo.com",
              "pathPrefix": "/offer-mobile"
            },
            {
              "scheme": "https",
              "host": "app.gosholo.com",
              "pathPrefix": "/event-mobile"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app uses location to show nearby restaurants and food events on the map."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZ29zaG9sb2RldiIsImEiOiJjbWU5NWptYXMwbHdqMmttcXVxbTdhajNrIn0.l0kt4ycQbI8Qiz2s_77NNQ"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2bab544a-7b3d-4844-8a22-4d92909ec243"
      }
    }
  }
}
